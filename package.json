{
  "name": "df-tools",
  "version": "0.0.1",
  "description": "Dwarf Fortress related tools",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "mocha": "mocha --recursive --compilers js:babel-register \"test/**/test-*.js\"",
    "mocha:integration": "mocha --recursive --compilers js:babel-register \"test/**/int-*.js\"",
    "mocha:integration:debug": "mocha --debug-brk --inspect --recursive --compilers js:babel-register \"test/**/int-*.js\"",
    "mocha:watch": "nodemon",
    "lint": "eslint \"**/*.js\" && echo Lint passed.",
    "coverage": "nyc npm run mocha",
    "build": "babel src -d dist",
    "dev": "babel src --watch -d dist",
    "dev:cli": "babel-node src/cli/index.js",
    "dev:cli:debug": "babel-node --inspect --debug-brk src/cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthemsteger/df-tools.git"
  },
  "keywords": [
    "dwarf fortress"
  ],
  "author": "Matt Hemsteger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matthemsteger/df-tools/issues"
  },
  "homepage": "https://github.com/matthemsteger/df-tools#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chai-interface": "^2.0.3",
    "chai-sinon": "^2.8.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^3.5.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.1.2",
    "proxyquire": "^1.7.11",
    "sinon": "^2.3.8"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "chevrotain": "^0.28.2",
    "debug": "^2.6.3",
    "dustjs-helpers": "^1.7.3",
    "dustjs-linkedin": "^2.7.5",
    "glob": "^7.1.1",
    "iconv-lite": "^0.4.15",
    "lodash": "^4.17.4",
    "parsimmon": "^1.6.2",
    "ramda": "^0.24.1",
    "yargs": "^7.0.2"
  }
}
