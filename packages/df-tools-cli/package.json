{
	"name": "@df-tools/df-tools-cli",
	"version": "0.0.1",
	"description": "The cli for df-tools packages",
	"main": "dist/index.js",
	"bin": {
		"df-tools": "dist/cli.js"
	},
	"scripts": {
		"test": "npm run mocha",
		"test:coverage": "nyc --cache npm run mocha && nyc report --reporter=lcov",
		"mocha": "mocha --recursive -r ../../babel.register.js \"test/**/*.spec.js\"",
		"mocha:watch": "mocha --recursive --reporter min -r ../../babel.register.js --watch \"test/**/*.spec.js\"",
		"lint": "eslint \"**/*.js\" && echo Lint passed.",
		"build": "babel --root-mode upward src -d dist",
		"dev": "babel src --watch -d dist",
		"prepublishOnly": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matthemsteger/df-tools.git"
	},
	"keywords": [
		"dwarf fortress"
	],
	"author": "Matt Hemsteger",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/matthemsteger/df-tools/issues"
	},
	"homepage": "https://github.com/matthemsteger/df-tools#readme",
	"engines": {
		"node": ">= 12.4.0"
	},
	"dependencies": {
		"@df-tools/df-tools-worldgen": "^0.0.1",
		"debug": "^4.0.1",
		"fluture": "^12.3.1",
		"ramda": "^0.27.0",
		"yargs": "^15.4.0"
	}
}
