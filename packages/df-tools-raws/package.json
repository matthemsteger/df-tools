{
	"name": "@df-tools/df-tools-raws",
	"version": "0.0.1",
	"description": "Dwarf Fortress tools for reading, writing and manipulating raw files and raw like token languages",
	"main": "dist/index.js",
	"scripts": {
		"test": "npm run mocha",
		"test:coverage": "nyc --cache npm run mocha && nyc report --reporter=lcov",
		"mocha": "mocha --recursive -r babel-register \"test/**/*.spec.js\"",
		"mocha:watch": "mocha --recursive --reporter min -r babel-register --watch \"test/**/*.spec.js\"",
		"lint": "eslint \"**/*.js\" && echo Lint passed.",
		"build": "babel src -d dist",
		"dev": "babel src --watch -d dist",
		"prepublishOnly": "npm run build",
		"dev:generate-raw-defs": "node dist/cli",
		"test:integration": "mocha --recursive -r babel-register \"test/**/*.int.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matthemsteger/df-tools.git"
	},
	"keywords": [
		"dwarf fortress"
	],
	"author": "Matt Hemsteger",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/matthemsteger/df-tools/issues"
	},
	"homepage": "https://github.com/matthemsteger/df-tools#readme",
	"engines": {
		"node": ">= 8.9"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-register": "^6.26.0",
		"chai": "^4.1.2",
		"chai-interface": "^2.0.3",
		"mocha": "^5.2.0",
		"nyc": "^12.0.2",
		"proxyquire": "^2.0.1",
		"sinon": "^6.0.0",
		"sinon-chai": "^3.2.0",
		"yargs": "^12.0.2"
	},
	"dependencies": {
		"@matthemsteger/utils-fn-fs": "^1.0.2",
		"@matthemsteger/utils-fn-futures": "^1.0.2",
		"fluture": "^10.1.0",
		"folktale": "^2.3.0",
		"parsimmon": "^1.12.0",
		"ramda": "^0.25.0"
	}
}
